
Host *.sli-systems.net *.sli.io *.globalbrain.net
  LogLevel ERROR
  PubkeyAcceptedKeyTypes +ssh-dss
  ServerAliveInterval 60
  StrictHostKeyChecking no
  UserKnownHostsFile /dev/null

Host *.sli-systems.net *.sli.io *.globalbrain.net
  User gbrain
  IdentityFile ~/.ssh/id_gbrain_dsa

Host *.sli-systems.net *.sli.io
  User root
  ProxyCommand ssh -o ForwardAgent=yes gbrain@mcbain.globalbrain.net 'ssh-add ~gbrain/search_production_root/identity_root &> /dev/null && nc %h %p'
