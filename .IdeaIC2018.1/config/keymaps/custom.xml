<keymap version="1" name="custom" parent="Default for XWin">
  <action id="$LRU">
    <keyboard-shortcut first-keystroke="ctrl alt e" />
  </action>
  <action id="Diff.ApplyLeftSide">
    <keyboard-shortcut first-keystroke="ctrl alt period" />
  </action>
  <action id="Diff.ApplyRightSide">
    <keyboard-shortcut first-keystroke="ctrl alt comma" />
  </action>
  <action id="Editor LRU">
    <keyboard-shortcut first-keystroke="ctrl alt e" />
  </action>
  <action id="EditorDown">
    <keyboard-shortcut first-keystroke="down" />
    <keyboard-shortcut first-keystroke="alt n" />
  </action>
  <action id="EditorSelectWord">
    <keyboard-shortcut first-keystroke="ctrl w" />
    <keyboard-shortcut first-keystroke="alt w" />
  </action>
  <action id="EditorUnSelectWord">
    <keyboard-shortcut first-keystroke="shift ctrl w" />
    <keyboard-shortcut first-keystroke="shift alt w" />
  </action>
  <action id="EditorUp">
    <keyboard-shortcut first-keystroke="up" />
    <keyboard-shortcut first-keystroke="alt p" />
  </action>
  <action id="RecentChangedFiles">
    <keyboard-shortcut first-keystroke="shift ctrl e" />
    <keyboard-shortcut first-keystroke="shift alt e" />
  </action>
  <action id="RecentChanges" />
  <action id="RecentFiles">
    <keyboard-shortcut first-keystroke="ctrl e" />
    <keyboard-shortcut first-keystroke="alt e" />
  </action>
  <action id="Unwrap">
    <keyboard-shortcut first-keystroke="shift ctrl delete" />
    <keyboard-shortcut first-keystroke="alt r" />
  </action>
</keymap>
