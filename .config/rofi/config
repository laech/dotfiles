rofi.kb-move-front: Control+Alt+h,Home
rofi.kb-move-end: Control+Alt+l,End
rofi.kb-move-word-back: Alt+h,Control+Left,Alt+Left
rofi.kb-move-word-forward: Alt+l,Control+Right,Alt+Right
rofi.kb-move-char-back: Control+h,Left
rofi.kb-move-char-forward: Control+l,Right
rofi.kb-remove-char-back: BackSpace
rofi.kb-remove-word-back: Control+BackSpace,Alt+BackSpace
rofi.kb-remove-word-forward: Alt+d
rofi.kb-remove-to-eol: Control+Alt+d
rofi.kb-remove-to-sol: Control+Alt+BackSpace
rofi.kb-accept-entry: Control+m,Return,KP_Enter
rofi.kb-row-up: Up,Control+k
