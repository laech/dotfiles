general {
  output_format = "i3bar"
  colors = true
  color_good = "#ffffff"
  color_degraded = "#555555"
  color_bad = "#555555"
  separator = ""
  interval = 10
}

order += "ethernet _first_"
order += "volume master"
order += "time"

ethernet _first_ {
  format_up = ""
  format_down = ""
}

volume master {
  format = ""
  format_muted = ""
  device = "default"
  mixer = "Master"
  mixer_idx = 0
}

time {
  format = "%F %I:%M %p"
}
